static bool write_file(const char* file, const char* what, ...)
{
  char buf[1024];
  va_list args;
  va_start(args, what);
  vsnprintf(buf, sizeof(buf), what, args);
  va_end(args);
  buf[sizeof(buf) - 1] =3D 0;
  int len =3D strlen(buf);
  int fd =3D open(file, O_WRONLY | O_CLOEXEC);
  if (fd =3D=3D -1)
    return false;
  if (write(fd, buf, len) !=3D len) {
    int err =3D errno;
    close(fd);
    errno =3D err;
    return false;
  }
  close(fd);
  return true;
}

