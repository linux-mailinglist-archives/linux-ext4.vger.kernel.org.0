+# Makefile for pin pages driver and test application
+
+# Kernel build directory, set this to kernel source or headers
+KDIR := ""
+
+# Driver source
+DRIVER_NAME := pin_pages
+obj-m := $(DRIVER_NAME).o
+$(DRIVER_NAME)-objs := pin.o
+
+# Test application
+TEST_APP := test_pin
+TEST_SRC := test_pin.c
+
+# Compiler flags for userspace application
+CFLAGS := -Wall -Wextra -std=c99
+
+.PHONY: all clean driver app load unload test
+
+all: driver app
+
+# Build kernel driver
+driver:
+	make -C $(KDIR) M=$(PWD) modules
+
+# Build userspace test application
+app: $(TEST_APP)
+
+$(TEST_APP): $(TEST_SRC)
+	gcc $(CFLAGS) -o $(TEST_APP) $(TEST_SRC)
+
+# Load the driver module
+load: driver
+	sudo insmod $(DRIVER_NAME).ko
+
+# Unload the driver module
+unload:
+	sudo rmmod $(DRIVER_NAME)
+
+# Run the test (requires driver to be loaded)
+test: app
+	sudo ./$(TEST_APP)
+
+# Clean build artifacts
+clean:
+	make -C $(KDIR) M=$(PWD) clean
+	rm -f $(TEST_APP)
+
+help:
+	@echo "Available targets:"
+	@echo "  all     - Build both driver and test application"
+	@echo "  driver  - Build kernel driver only"
+	@echo "  app     - Build test application only"
+	@echo "  load    - Load the driver module"
+	@echo "  unload  - Unload the driver module"
+	@echo "  test    - Run the test application (requires loaded driver)"
+	@echo "  clean   - Clean all build artifacts"
diff --git a/pin.c b/pin.c
new file mode 100644
index 0000000..436939b
--- /dev/null
+++ b/pin.c
@@ -0,0 +1,281 @@
+#include <linux/module.h>
+#include <linux/kernel.h>
+#include <linux/fs.h>
+#include <linux/device.h>
+#include <linux/cdev.h>
+#include <linux/uaccess.h>
+#include <linux/mm.h>
+#include <linux/slab.h>
+#include <linux/ioctl.h>
+#include <linux/list.h>
+#include <linux/mutex.h>
+
+#define DEVICE_NAME "pin_pages"
+#define CLASS_NAME "pin_pages_class"
+
+/* IOCTL definitions */
+#define PIN_PAGES_IOC_MAGIC 'p'
+#define PIN_PAGES_IOC_PIN   _IOW(PIN_PAGES_IOC_MAGIC, 1, struct pin_pages_request)
+#define PIN_PAGES_IOC_UNPIN _IO(PIN_PAGES_IOC_MAGIC, 2)
+
+/* Structure for IOCTL communication */
+struct pin_pages_request {
+    void __user *user_ptr;
+    size_t size;
+};
+
+/* Structure to track pinned pages */
+struct pin_data {
+    struct page **pages;
+    int nr_pages;
+    unsigned long start_addr;
+    size_t size;
+    struct list_head list;
+};
+
+/* Per-file data structure */
+struct file_data {
+    struct list_head pin_list;
+    struct mutex pin_mutex;
+};
+
+static int major_number;
+static struct class *pin_pages_class = NULL;
+static struct device *pin_pages_device = NULL;
+static struct cdev pin_pages_cdev;
+
+static int device_open(struct inode *inode, struct file *file)
+{
+    struct file_data *fdata;
+
+    pr_info("pin_pages_driver: Device opened\n");
+
+    fdata = kzalloc(sizeof(*fdata), GFP_KERNEL);
+    if (!fdata)
+        return -ENOMEM;
+
+    INIT_LIST_HEAD(&fdata->pin_list);
+    mutex_init(&fdata->pin_mutex);
+    file->private_data = fdata;
+
+    pr_info("pin_pages_driver: File data initialized\n");
+
+    return 0;
+}
+
+static void cleanup_pinned_pages(struct file_data *fdata)
+{
+    struct pin_data *pdata, *tmp;
+
+    list_for_each_entry_safe(pdata, tmp, &fdata->pin_list, list) {
+        if (pdata->pages) {
+            unpin_user_pages(pdata->pages, pdata->nr_pages);
+            kfree(pdata->pages);
+        }
+        list_del(&pdata->list);
+        kfree(pdata);
+    }
+}
+
+static int device_release(struct inode *inode, struct file *file)
+{
+    struct file_data *fdata = file->private_data;
+
+    pr_info("pin_pages_driver: Device closed\n");
+
+    if (fdata) {
+	pr_info("pin_pages_driver: Cleaning up pinned pages\n");
+        mutex_lock(&fdata->pin_mutex);
+        cleanup_pinned_pages(fdata);
+        mutex_unlock(&fdata->pin_mutex);
+        mutex_destroy(&fdata->pin_mutex);
+        kfree(fdata);
+	pr_info("pin_pages_driver: File data freed\n");
+    }
+
+
+    return 0;
+}
+
+static long device_ioctl(struct file *file, unsigned int cmd, unsigned long arg)
+{
+    struct file_data *fdata = file->private_data;
+    struct pin_pages_request req;
+    struct pin_data *pdata;
+    struct vm_area_struct *vma;
+    unsigned long start_page, end_page;
+    int nr_pages;
+    int ret;
+
+    if (!fdata)
+        return -EINVAL;
+
+    printk("pin_pages_driver: IOCTL command received: 0x%x\n", cmd);
+
+    if (_IOC_TYPE(cmd) != PIN_PAGES_IOC_MAGIC)
+        return -ENOTTY;
+
+    switch (cmd) {
+    case PIN_PAGES_IOC_PIN:
+        if (copy_from_user(&req, (void __user *)arg, sizeof(req)))
+            return -EFAULT;
+
+        if (!req.user_ptr || !req.size)
+            return -EINVAL;
+
+        start_page = (unsigned long)req.user_ptr & PAGE_MASK;
+        end_page = ((unsigned long)req.user_ptr + req.size - 1) & PAGE_MASK;
+        nr_pages = (end_page - start_page) / PAGE_SIZE + 1;
+
+        /* Find and modify VMA */
+        mmap_write_lock(current->mm);
+        vma = find_vma(current->mm, (unsigned long)req.user_ptr);
+        if (!vma || vma->vm_start > (unsigned long)req.user_ptr) {
+            mmap_read_unlock(current->mm);
+            pr_err("pin_pages_driver: VMA not found for address 0x%lx\n", 
+                   (unsigned long)req.user_ptr);
+            return -EFAULT;
+        }
+
+        /* Check if the entire range is within this VMA */
+        if (vma->vm_end < (unsigned long)req.user_ptr + req.size) {
+            mmap_read_unlock(current->mm);
+            pr_err("pin_pages_driver: Address range spans multiple VMAs\n");
+            return -EFAULT;
+        }
+
+        /* Clear VM_MAYWRITE flag */
+        pr_info("pin_pages_driver: VMA flags before: 0x%lx\n", vma->vm_flags);
+
+	// Everything works without CoW flags
+	//vm_flags_clear(vma, VM_MAYWRITE); 
+        pr_info("pin_pages_driver: VMA flags after: 0x%lx\n", vma->vm_flags);
+        mmap_write_unlock(current->mm);
+
+        /* Allocate pin_data structure */
+        pdata = kzalloc(sizeof(*pdata), GFP_KERNEL);
+        if (!pdata)
+            return -ENOMEM;
+
+        /* Allocate array for page pointers */
+        pdata->pages = kmalloc_array(nr_pages, sizeof(struct page *), GFP_KERNEL);
+        if (!pdata->pages) {
+            kfree(pdata);
+            return -ENOMEM;
+        }
+
+        /* Pin the user pages */
+        ret = pin_user_pages_fast((unsigned long)req.user_ptr, nr_pages,
+                                  FOLL_LONGTERM, pdata->pages);
+        if (ret < 0) {
+            kfree(pdata->pages);
+            kfree(pdata);
+            return ret;
+        }
+
+        if (ret != nr_pages) {
+            /* Partial pin - unpin what we got and fail */
+            unpin_user_pages(pdata->pages, ret);
+            kfree(pdata->pages);
+            kfree(pdata);
+            return -EFAULT;
+        }
+
+        pdata->nr_pages = nr_pages;
+        pdata->start_addr = (unsigned long)req.user_ptr;
+        pdata->size = req.size;
+
+        /* Add to the file's pin list */
+        mutex_lock(&fdata->pin_mutex);
+        list_add(&pdata->list, &fdata->pin_list);
+        mutex_unlock(&fdata->pin_mutex);
+
+        pr_info("pin_pages_driver: Pinned %d pages starting at 0x%lx\n",
+                nr_pages, (unsigned long)req.user_ptr);
+        break;
+
+    case PIN_PAGES_IOC_UNPIN:
+        mutex_lock(&fdata->pin_mutex);
+        cleanup_pinned_pages(fdata);
+        mutex_unlock(&fdata->pin_mutex);
+        pr_info("pin_pages_driver: Unpinned all pages\n");
+        break;
+
+    default:
+        return -ENOTTY;
+    }
+
+    return 0;
+}
+
+static const struct file_operations fops = {
+    .open = device_open,
+    .release = device_release,
+    .unlocked_ioctl = device_ioctl,
+    .compat_ioctl = device_ioctl,
+};
+
+static int __init pin_pages_init(void)
+{
+    dev_t dev;
+    int ret;
+
+    /* Allocate major number */
+    ret = alloc_chrdev_region(&dev, 0, 1, DEVICE_NAME);
+    if (ret < 0) {
+        pr_err("pin_pages_driver: Failed to allocate major number\n");
+        return ret;
+    }
+    major_number = MAJOR(dev);
+
+    /* Initialize cdev */
+    cdev_init(&pin_pages_cdev, &fops);
+    pin_pages_cdev.owner = THIS_MODULE;
+
+    /* Add cdev to kernel */
+    ret = cdev_add(&pin_pages_cdev, dev, 1);
+    if (ret) {
+        unregister_chrdev_region(dev, 1);
+        pr_err("pin_pages_driver: Failed to add cdev\n");
+        return ret;
+    }
+
+    /* Create device class */
+    pin_pages_class = class_create(CLASS_NAME);
+    if (IS_ERR(pin_pages_class)) {
+        cdev_del(&pin_pages_cdev);
+        unregister_chrdev_region(dev, 1);
+        pr_err("pin_pages_driver: Failed to create class\n");
+        return PTR_ERR(pin_pages_class);
+    }
+
+    /* Create device */
+    pin_pages_device = device_create(pin_pages_class, NULL, dev, NULL, DEVICE_NAME);
+    if (IS_ERR(pin_pages_device)) {
+        class_destroy(pin_pages_class);
+        cdev_del(&pin_pages_cdev);
+        unregister_chrdev_region(dev, 1);
+        pr_err("pin_pages_driver: Failed to create device\n");
+        return PTR_ERR(pin_pages_device);
+    }
+
+    pr_info("pin_pages_driver: Device created successfully\n");
+    return 0;
+}
+
+static void __exit pin_pages_exit(void)
+{
+    device_destroy(pin_pages_class, MKDEV(major_number, 0));
+    class_destroy(pin_pages_class);
+    cdev_del(&pin_pages_cdev);
+    unregister_chrdev_region(MKDEV(major_number, 0), 1);
+    pr_info("pin_pages_driver: Device removed successfully\n");
+}
+
+module_init(pin_pages_init);
+module_exit(pin_pages_exit);
+
+MODULE_LICENSE("GPL");
+MODULE_AUTHOR("Karol Wachowski");
+MODULE_DESCRIPTION("Simple driver to prove pinning user pages works");
+MODULE_VERSION("1.0");
diff --git a/test_pin.c b/test_pin.c
new file mode 100644
index 0000000..b94d01a
--- /dev/null
+++ b/test_pin.c
@@ -0,0 +1,158 @@
+#include <fcntl.h>
+#include <stdio.h>
+#include <stdlib.h>
+#include <string.h>
+#include <sys/ioctl.h>
+#include <sys/mman.h>
+#include <sys/stat.h>
+#include <unistd.h>
+
+#define DEVICE_PATH "/dev/pin_pages"
+#define TEST_FILE_PATH "/tmp/test_file"
+#define TEST_DATA_SIZE (2 * 1024 * 1024)
+
+/* IOCTL definitions - must match the kernel driver */
+#define PIN_PAGES_IOC_MAGIC 'p'
+#define PIN_PAGES_IOC_PIN _IOW(PIN_PAGES_IOC_MAGIC, 1, struct pin_pages_request)
+#define PIN_PAGES_IOC_UNPIN _IO(PIN_PAGES_IOC_MAGIC, 2)
+
+/* Structure for IOCTL communication - must match kernel driver */
+struct pin_pages_request {
+	void *user_ptr;
+	size_t size;
+};
+
+int create_test_file(const char *path, size_t size) {
+	int fd;
+	char *buffer;
+
+	fd = open(path, O_CREAT | O_RDWR | O_TRUNC, 0644);
+	if (fd < 0) {
+		perror("Failed to create test file");
+		return -1;
+	}
+
+	/* Create buffer with test data */
+	buffer = malloc(size);
+	if (!buffer) {
+		perror("Failed to allocate buffer");
+		close(fd);
+		return -1;
+	}
+
+	/* Fill with test pattern */
+	for (size_t i = 0; i < size; i++) {
+		buffer[i] = (char)(i % 256);
+	}
+
+	/* Write to file */
+	if (write(fd, buffer, size) != (ssize_t)size) {
+		perror("Failed to write test data");
+		free(buffer);
+		close(fd);
+		return -1;
+	}
+
+	free(buffer);
+	close(fd);
+	return 0;
+}
+
+int main() {
+	int driver_fd, file_fd;
+	void *mapped_addr;
+	struct pin_pages_request req;
+	int ret = 0;
+
+	printf("Pin Pages Driver Test Application\n");
+	printf("==================================\n");
+
+	/* Create test file */
+	printf("Creating test file: %s\n", TEST_FILE_PATH);
+	if (create_test_file(TEST_FILE_PATH, TEST_DATA_SIZE) < 0) {
+		fprintf(stderr, "Failed to create test file\n");
+		return 1;
+	}
+
+	/* Open test file for mmap */
+	file_fd = open(TEST_FILE_PATH, O_RDWR);
+	if (file_fd < 0) {
+		perror("Failed to open test file");
+		ret = 1;
+		goto cleanup_file;
+	}
+
+	/* Map the file into memory */
+	mapped_addr = mmap(NULL, TEST_DATA_SIZE, PROT_READ, MAP_PRIVATE, file_fd, 0);
+	if (mapped_addr == MAP_FAILED) {
+		perror("Failed to mmap test file");
+		ret = 1;
+		goto cleanup_fd;
+	}
+
+	printf("Mapped file at address: %p\n", mapped_addr);
+
+	/* Verify mapped data */
+	printf("First few bytes of mapped data: ");
+	for (int i = 0; i < 16; i++) {
+		printf("%02x ", ((unsigned char *)mapped_addr)[i]);
+	}
+	printf("\n");
+
+	/* Open the pin pages device */
+	driver_fd = open(DEVICE_PATH, O_RDWR);
+	if (driver_fd < 0) {
+		perror("Failed to open pin pages device");
+		printf(
+		    "Make sure the kernel module is loaded and device "
+		    "exists\n");
+		ret = 1;
+		goto cleanup_mmap;
+	}
+
+	printf("Opened pin pages device successfully\n");
+
+	/* Prepare IOCTL request */
+	req.user_ptr = mapped_addr;
+	req.size = TEST_DATA_SIZE;
+
+	/* Pin the pages */
+	printf("Pinning %zu bytes at address %p\n", req.size, req.user_ptr);
+	if (ioctl(driver_fd, PIN_PAGES_IOC_PIN, &req) < 0) {
+		perror("Failed to pin pages");
+		ret = 1;
+		goto cleanup_driver;
+	}
+
+	printf("Pages pinned successfully!\n");
+
+	/* Unpin the pages */
+	printf("Unpinning pages\n");
+	if (ioctl(driver_fd, PIN_PAGES_IOC_UNPIN, 0) < 0) {
+		perror("Failed to unpin pages");
+		ret = 1;
+	} else {
+		printf("Pages unpinned successfully!\n");
+	}
+
+cleanup_driver:
+	close(driver_fd);
+
+cleanup_mmap:
+	if (munmap(mapped_addr, TEST_DATA_SIZE) < 0) {
+		perror("Failed to unmap memory");
+		ret = 1;
+	}
+
+cleanup_fd:
+	close(file_fd);
+
+cleanup_file:
+	/* Remove test file */
+	if (unlink(TEST_FILE_PATH) < 0) {
+		perror("Failed to remove test file");
+	}
+
+	printf("Test completed with %s\n", ret == 0 ? "SUCCESS" : "FAILURE");
+	return ret;
+}
-- 
2.43.0


